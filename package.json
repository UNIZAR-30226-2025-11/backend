{
  "name": "katboom-backend",
  "version": "0.0.1",
  "description": "Backend for KatBoom",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/",
    "start": "node --env-file=.env dist/index.js",
    "dev": "tsx --env-file=.env src/index.ts",
    "watch": "tsx watch --env-file=.env src/index.ts",
    "test": "vitest",
    "lint": "eslint src/",
    "format": "prettier src/ test/ --write",
    "fix": "eslint --fix src/ test/",
    "docker:build": "docker build . --file docker/Dockerfile",
    "compose:build": "docker compose -p katboom -f docker/docker-compose.yml build",
    "compose:up": "docker compose -p katboom -f docker/docker-compose.yml up",
    "compose:down": "docker compose -p katboom -f docker/docker-compose.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.7",
    "eslint-config-prettier": "^10.0.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.2",
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "socket.io": "^4.8.1"
  }
}
